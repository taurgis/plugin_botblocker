<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-profile service-profile-id="BotBlocker">
        <timeout-millis>1000</timeout-millis>
        <rate-limit-enabled>true</rate-limit-enabled>
        <rate-limit-calls>1</rate-limit-calls>
        <rate-limit-millis>10000</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>5</cb-calls>
        <cb-millis>1000</cb-millis>
    </service-profile>

    <service service-id="BotBlocker.BlacklistService">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>botblocker</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>BotBlocker</profile-id>
        <credential-id/>
    </service>

    <service service-id="BotBlocker.IPInformationService">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>botblockerip</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>false</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>BotBlocker</profile-id>
        <credential-id/>
    </service>

</services>
