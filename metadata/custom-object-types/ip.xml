<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
   
    <custom-type type-id="BotBlocker_IP">
        <display-name xml:lang="x-default">Botblocker IPs</display-name>
        <description xml:lang="x-default">The current IPs being tracked by the Bot Blocker plugin (when enabled through site preference).</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <retention-days>7</retention-days>
        <key-definition attribute-id="IP">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="age">
                <display-name xml:lang="x-default">Age</display-name>
                <description xml:lang="x-default">The number of seconds it was since first request that caused the blacklist.</description>
                <type>double</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="count">
                <display-name xml:lang="x-default">Count</display-name>
                <description xml:lang="x-default">The number of requests that triggered the ban.</description>
                <type>double</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="userAgent">
                <display-name xml:lang="x-default">User Agent</display-name>
                <description xml:lang="x-default">The User Agent used to make the request</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-height>20</field-height>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="main">
                <display-name xml:lang="x-default">Main</display-name>
                <attribute attribute-id="IP"/>
                <attribute attribute-id="userAgent"/>
                <attribute attribute-id="count"/>
                <attribute attribute-id="age"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
